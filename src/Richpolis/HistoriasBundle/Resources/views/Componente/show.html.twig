{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Componente</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Tipo</th>
                <td>{{ entity.tipo }}</td>
            </tr>
            <tr>
                <th>Componente</th>
                <td>{{ entity.componente }}</td>
            </tr>
            <tr>
                <th>Tipousuario</th>
                <td>{{ entity.tipoUsuario }}</td>
            </tr>
            <tr>
                <th>Position</th>
                <td>{{ entity.position }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('componentes') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('componentes_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
